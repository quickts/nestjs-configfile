    "license": "MIT",
    "devDependencies": {
        "@nestjs/common": "^7.4.4",
        "@nestjs/core": "^7.4.4",
        "@nestjs/platform-express": "^7.4.4",
        "@types/js-yaml": "^3.12.5",
        "@types/mocha": "^8.0.3",
        "@types/node": "^14.10.2",
        "mocha": "^8.1.3",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.3",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.2"
    },
    "dependencies": {
